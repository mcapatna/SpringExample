<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="hai" class="com.mcapatna.spring.ioc.model.Hai">
		<property name="msg" value="his is msg from Hi" />
		<property name="no" value="12" />
	</bean>
	<bean id="aobj" class="com.mcapatna.spring.ioc.model.A">
		<property name="msg" value="This is msg from A" />
	</bean>
	<bean id="hello" class="com.mcapatna.spring.ioc.model.Hello"
		autowire="byName">
	</bean>
</beans>