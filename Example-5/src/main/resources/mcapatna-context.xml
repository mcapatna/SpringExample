<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="aobj" class="com.mcapatna.spring.ioc.model.A">
		<property name="msg" value="Msg from A" />
	</bean>
	<bean id="ha1" class="com.mcapatna.spring.ioc.model.Hai">
		<property name="msg" value="Msg from Hai" />
		<property name="n" value="99" />
	</bean>
	<bean id="hello" class="com.mcapatna.spring.ioc.model.Hello"
	autowire="constructor">
	</bean>
</beans>