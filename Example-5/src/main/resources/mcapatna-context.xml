<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="indianCricket" class="com.mcapatna.spring.ioc.model.Cricket">
		<property name="captain" value="'MSD'" />
	</bean>
	<bean id="indianHockey" class="com.mcapatna.spring.ioc.model.Hockey">
		<property name="captain" value="'Sardara Singh'" />
		<property name="totalGoal" value="299" />
	</bean>
	<bean id="indianGames" class="com.mcapatna.spring.ioc.model.IndianGames"
	autowire="constructor">
	</bean>
</beans>