<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="ao" class="com.mcapatna.spring.ioc.model.Foo">
		<property name="a" value="111" />
		<property name="str" value="a" />
	</bean>
	<bean id="bo" class="com.mcapatna.spring.ioc.model.Bar">
		<constructor-arg value="12" />
		<constructor-arg value="oops" />
	</bean>
	<bean id="hello" class="com.mcapatna.spring.ioc.app.AppMgr">
		<property name="aobj" ref="ao"/>
		<property name="bobj" ref="bo"/>
	</bean>

</beans>